{
  "appId": "com.iptvzeus.app",
  "appName": "IPTV Zeus",
  "webDir": "build",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https",
    "cleartext": true,
    "allowNavigation": [
      "*"
    ]
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true
  },
  "ios": {
    "limitsNavigationsToAppBoundDomains": false
  }
}
